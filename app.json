{
  "expo": {
    "name": "PhiCam",
    "slug": "phicam",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.1",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.moreyes.phicam",
      "requireFullScreen": true
    },
    "android": {
      "package": "com.moreyes.phicam"
    },
    "description": "Explore the world through PHI RATIO (Golden Ratio)"
  }
}
